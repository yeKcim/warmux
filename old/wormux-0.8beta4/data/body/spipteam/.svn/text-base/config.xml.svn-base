	<anchor dx="16" dy="17"/>
	<attached member_type="hoop" frame="*" dx="10" dy="22"/>
	<attached member_type="head" frame="*" dx="11" dy="6"/>
	<attached member_type="arm-right"  frame="*" dx="6" dy="15"/>
