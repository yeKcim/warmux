<!-- ###### Yoyo ###### -->
<movement name="animation0" speed="5400" play_mode="play_once" > <!-- 5400 = 36 (frames of yoyo animation) * 150ms (speed of the yoyos animation) -->
	<collision_rect left="5" right="5" top="10" bottom="2"/>
